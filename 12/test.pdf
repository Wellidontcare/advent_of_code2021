// Caves
digraph {
	graph [splines=ortho]
	start -> A [arrowhead=both]
	A -> start [arrowhead=both]
	start -> b [arrowhead=both]
	b -> start [arrowhead=both]
	b -> A [arrowhead=both]
	A -> c [arrowhead=both]
	c -> A [arrowhead=both]
	A -> b [arrowhead=both]
	b -> d [arrowhead=both]
	d -> b [arrowhead=both]
	b -> end [arrowhead=both]
	end -> A [arrowhead=both]
	A -> end [arrowhead=both]
	end -> b [arrowhead=both]
}
